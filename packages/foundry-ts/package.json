{
  "name": "@scaffold-eth/foundry-ts",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "chain": "cross-env BUILDING=true anvil --balance 10000000 --host 0.0.0.0 ",
    "fork": "cross-env BUILDING=true hardhat node --fork https://mainnet.infura.io/v3/460f40a260564ac4a4f4b3fffb032dad",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register test/**/*.test.ts",
    "test:watch": "TS_NODE_TRANSPILE_ONLY=true NODE_ENV=test mocha --require ts-node/register -r tsconfig-paths/register  --watch-extensions ts --watch --watch-files  test 'test/**/**.test.ts'",
    "compile": "cross-env BUILDING=true hardhat compile",
    "deploy": "rm -rf rm -rf generated/deployments && cross-env BUILDING=true hardhat deploy --export-all ../next-ts/contracts/hardhat_contracts.json",
    "clean": "cross-env BUILDING=true hardhat clean",
    "deploy:post": "hardhat run scripts/publish.ts",
    "watch:compile": "yarn hardhat watch compile",
    "accounts": "hardhat accounts",
    "generate": "hardhat generate",
    "account": "hardhat account",
    "docs": "hardhat dodocs",
    "verify:etherscan": "hardhat etherscan-verify --api-key KP6ZGYIWNJ5XK2NKJK8N477A7XKMU75C2D",
    "verify:tenderly": "hardhat tenderly-verify",
    "lint": "eslint --config ./.eslintrc --ignore-path ./.eslintignore ./deploy/**/*.ts ./tasks/**/*.ts ./test/**/*.ts ./helpers/**/*.ts",
    "solhint": "solhint ./src/**/*.sol",
    "format": "yarn prettier -w ./deploy/**/*.ts ./tasks/**/*.ts ./test/**/*.ts ./helpers/**/*.ts src/**/*.sol ",
    "format:check": "yarn prettier -c ./deploy/**/*.ts ./tasks/**/*.ts ./test/**/*.ts ./helpers/**/*.ts ./src/**/*.sol ",
    "hh:watch-test": "hardhat watch compilation & hardhat watch test --network hardhat",
    "--------": "--------------Forge Commands---------------",
    "forge:install": "forge install  --no-commit ",
    "forge:build": "forge build && yarn generate-types ",
    "forge:watch": "forge build --watch ",
    "forge:clean": "forge clean ",
    "forge:test": "forge test ",
    "forge:watch-test": "forge test --watch -vvv",
    "forge:generate": "ts-node  --ignore 'generated/*' scripts/generate.ts",
    "forge:account": "ts-node scripts/account.ts",
    "forge:deploy": "cross-env ETHERSCAN_API_KEY=KP6ZGYIWNJ5XK2NKJK8N477A7XKMU75C2D ts-node --ignore 'out/*.json'   scripts/deploy.ts ",
    "generate-types": "typechain  --discriminate-types --target=ethers-v5 'out/**/*.json' --out-dir=./generated/contract-types ",
    "foundry:install": "chmod +x foundry-init.mjs && zx foundry-init.mjs",
    "foundry:help": "chmod +x help.mjs && zx help.mjs"
  },
  "dependencies": {
    "@atixlabs/hardhat-time-n-mine": "^0.0.5",
    "@defi-wonderland/smock": "^2.0.7",
    "@eth-optimism/hardhat-ovm": "^0.2.4",
    "@ethereum-waffle/ens": "^3.4.4",
    "@ethereumjs/vm": "^5.9.0",
